   "source": [
    "def recommend(song):\n",
    "    try:\n",
    "        index = finalsongs[finalsongs['track_name'] == song].index[0]\n",
    "        distances = similarity[index]\n",
    "        recommendations = sorted(list(enumerate(distances)), reverse=True, key=lambda x: x[1])[1:6]\n",
    "        \n",
    "        print(f\"Recommendations for '{song}':\")\n",
    "        for i in recommendations:\n",
    "            print(f\"- {finalsongs.iloc[i[0]]['track_name']} (Similarity: {i[1]:.2f})\")\n",
    "    except IndexError:\n",
    "        print(f\"Song '{song}' not found in the dataset.\")"
   ]
