   "source": [
    "# Generate top-5 recommendations for each song\n",
    "top_k = 10\n",
    "recommendations = {}\n",
    "for i in range(similarity.shape[0]):\n",
    "    top_indices = np.argsort(similarity[i])[-top_k-1:-1][::-1]  # Top-5 indices excluding itself\n",
    "    recommendations[i] = list(top_indices)\n",
    "\n",
    "# Save recommendations to a file (CSV or JSON)\n",
    "recommendations_df = pd.DataFrame.from_dict(recommendations, orient='index')\n",
    "recommendations_df.to_csv(\"song_recommendations.csv\", header=False)\n",
    "\n",
    "print(\"Recommendations file 'song_recommendations.csv' created successfully.\")"
   ]
